{
  "name": "@maxflow/client",
  "version": "0.2.8",
  "description": "Client library for Maxflow API",
  "keywords": [
    "maxflow",
    "ai",
    "client"
  ],
  "homepage": "https://github.com/maxflow-ai/maxflow-client-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxflow-ai/maxflow-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/maxflow-ai/maxflow-client-js/issues"
  },
  "author": {
    "name": "Maxflow",
    "email": "hello@maxflow.cloud",
    "url": "https://maxflow.cloud"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "prepare": "yarn build",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "eslint": "^8",
    "@types/node": "^18",
    "tsup": "^6.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
